# Frontend environment example for voice-tts-kr demo
#
# Copy this file to `.env` in the `frontend/tts-app/` directory and adjust as needed.

# Base URL for the TTS gateway backend.
# In local dev (backend on host): http://localhost:8080
# In Docker with ports forwarded 8080:8080: http://localhost:8080
VITE_TTS_API_BASE_URL=http://localhost:8080

VITE_STRESS_MAX_SESSIONS=10000
VITE_STRESS_MAX_CONCURRENCY=1000
VITE_STRESS_DEFAULT_SESSIONS=10000
VITE_STRESS_DEFAULT_CONCURRENCY=1000
VITE_METRICS_POLL_INTERVAL_MS=2000
